{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://github.com/whisk/skadnetwork/schema/v4.0",
  "title": "Apple SKAdNetwork Postback Version 4.0",
  "description": "",
  "type": "object",
  "properties": {
    "version": {
      "type": "string",
      "enum": [
        "4.0"
      ]
    },
    "ad-network-id": {
      "type": "string"
    },
    "source-identifier": {
      "type": "string",
      "minLength": 2,
      "maxLength": 4,
      "pattern": "^[0-9]+$"
    },
    "app-id": {
      "type": "integer",
      "minimum": 1
    },
    "transaction-id": {
      "type": "string",
      "format": "uuid"
    },
    "redownload": {
      "type": "boolean"
    },
    "source-domain": {
      "type": "string",
      "format": "hostname"
    },
    "fidelity-type": {
      "type": "integer",
      "minimum": 0,
      "maximum": 1
    },
    "did-win": {
      "type": "boolean"
    },
    "conversion-value": {
      "type": "integer",
      "minimum": 0,
      "maximum": 63
    },
    "coarse-conversion-value": {
      "type": "string",
      "enum": [
        "low",
        "medium",
        "high"
      ]
    },
    "postback-sequence-index": {
      "type": "integer",
      "minimum": 0,
      "maximum": 2
    },
    "attribution-signature": {
      "type": "string"
    }
  },
  "required": [
    "version",
    "ad-network-id",
    "source-identifier",
    "app-id",
    "transaction-id",
    "redownload",
    "fidelity-type",
    "did-win",
    "postback-sequence-index"
  ]
}
